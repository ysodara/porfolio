<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sodara Y Portfolio</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    

</head>
<body data-spy="scroll" data-target=".navbar" data-offset="50">

    <div class="navbar navbar-inverse navbar-fixed-top">
        
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>

            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a class="" href="#home">Home</a></li>
                    <li><a class="" href="#about">About</a></li>
                    <li><a class="" href="#project">Projects</a></li>
                    <li><a class="" href="#skill">Skills</a></li>
                    <li><a class="" href="#education">Edcuation</a></li>

                </ul>
                @*@Html.Partial("_LoginPartial")*@
            </div>

        
    </div>

    <div class="container-fluid body-content" style="margin:0; padding:0;">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Sodara Y Portfolio</p>
        </footer>
    </div>
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
<script src="/Scripts/main.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
